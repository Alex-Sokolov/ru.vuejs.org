<div class="sidebar">
    <ul class="main-menu">
        <%- partial('partials/main_menu', { context: 'sidebar' }) %>
    </ul>
    <div class="list">
        <div class="main-sponsor">
            <span>Платиновый спонсор</span><br>
            <a href="https://stdlib.com" target="_blank" class="logo">
                <img src="<%- url_for("/images/stdlib.png") %>">
            </a>
        </div>
        <a class="become-backer" href="<%- url_for("/support-vuejs") %>">
            Поддержите Vue.js
        </a>
        <h2>
            <% if (type === 'api') { %>
                API
            <% } if (type === 'guide') { %>
                Руководство
            <% } if (type === 'examples') { %>
                Примеры
            <% } %>
            <% if (type !== 'cookbook') { %>
                <select class="version-select">
                    <option value="SELF" selected>2.0</option>
                    <option value="v1">1.0</option>
                    <option value="012">0.12</option>
                    <option value="011">0.11</option>
                </select>
            <% } %>
        </h2>
        <%- partial('partials/toc', { type: type }) %>
    </div>
</div>
